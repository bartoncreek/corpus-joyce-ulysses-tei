   </desc><classes><memberOf key="att.global"/><memberOf key="att.typed"/></classes><content><sequence minOccurs="1" maxOccurs="1"><elementRef key="teiHeader"/><classRef key="model.resourceLike" minOccurs="1" maxOccurs="unbounded"/></sequence></content><constraintSpec ident="c1" scheme="schematron">
