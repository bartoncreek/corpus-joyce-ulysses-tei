      <memberOf key="model.common"/>
  </classes>
  
  
  <listRef>
      <ref target="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/ST.html#STEC">1.3. The TEI Class System</ref>
  </listRef>
</classSpec><classSpec module="tei" xml:id="COMMON" type="model" ident="model.common">
  <desc versionDate="2005-10-10" xml:lang="en">groups common chunk- and inter-level elements.</desc>
