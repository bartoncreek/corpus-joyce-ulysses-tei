          <desc versionDate="2008-03-30" xml:lang="fr">un pourcentage</desc>
          <desc versionDate="2007-01-21" xml:lang="it">percentuale</desc>
          <desc versionDate="2016-11-25" xml:lang="de">Prozentangabe</desc>
        </valItem>
